# Copy this file to `.env.local` (or `.env`) and fill values. Do NOT commit your real secrets.

# Gemini API Key (if used)
GEMINI_API_KEY=PLACEHOLDER_API_KEY

# Personal info
VITE_PERSONAL_NAME=Your Name
VITE_PERSONAL_TITLE="Your Title"
VITE_PERSONAL_LOCATION="City, Region"
VITE_PERSONAL_PHONE="+1 555 555 5555"
VITE_PERSONAL_EMAIL=your.email@example.com
VITE_PERSONAL_LINKEDIN=https://www.linkedin.com/in/your-profile
VITE_PERSONAL_GITHUB=https://github.com/your-username
VITE_PERSONAL_FREELANCING=true

# EmailJS / scheduler
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# Google Apps Script URL used by the scheduler (if applicable)
VITE_GOOGLE_SCRIPT_URL="https://script.google.com/macros/s/XXXXX/exec"
